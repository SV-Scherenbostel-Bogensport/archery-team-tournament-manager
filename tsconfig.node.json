{
  "extends": "@tsconfig/node22/tsconfig.json",
  "include": [
    "electron-vite.config.ts",
    "src/main/**/*.ts",
    "src/preload/**/*.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["node"]
  }
}
