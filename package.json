{
  "name": "archery-team-tournament-manager",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "build:win": "electron-vite build && electron-builder --win --config",
    "preview": "electron-vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^4.0.0",
    "@fontsource/roboto": "5.2.7",
    "@mdi/font": "7.4.47",
    "@tailwindcss/vite": "^4.1.18",
    "axios": "^1.13.5",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "vuedraggable": "4.1.0",
    "vuetify": "^3.10.1"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@types/vuedraggable": "^2.24.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.35.0",
    "eslint-config-vuetify": "^4.2.0",
    "npm-run-all2": "^8.0.4",
    "sass-embedded": "^1.92.1",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.2",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-fonts": "^1.4.0",
    "unplugin-vue-components": "^29.0.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.1.5",
    "vite-plugin-vue-layouts-next": "^1.0.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-tsc": "^3.2.0"
  },
  "build": {
    "appId": "dev.laubfrosch.archery.team-tournament-manager",
    "productName": "TeamTournamentManager",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "win": {
      "target": "portable",
      "artifactName": "${productName}_${version}.${ext}"
    },
    "files": [
      "out/**/*",
      "package.json"
    ]
  }
}
